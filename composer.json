    "type": "library",
    "require": {
        "illuminate/support": ">=10.0 <13.0",
        "illuminate/database": ">=10.0 <13.0",
        "illuminate/console": ">=10.0 <13.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Regions\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Regions\\RegionServiceProvider"
            ]
        }
    },
